<?xml version="1.0" encoding="utf-8"?>
<!--suppress DeprecatedClassUsageInspection -->
<TabHost xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@android:id/tabhost"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <LinearLayout
        android:weightSum="10"
        android:id="@+id/linearLayout1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <FrameLayout
            android:id="@android:id/tabcontent"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1">
            <com.github.matteobattilana.weather.WeatherView
                android:id="@+id/weather_view"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent">
            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:src="@drawable/line"
                android:id="@+id/ImView"/>

            <!-- Weather Now -->
            <LinearLayout
                android:id="@+id/weatherNow"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <!-- 첫 번째 화면 상단 -->
                <LinearLayout
                    android:layout_margin="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:padding="30dp"
                        android:orientation="vertical"
                        tools:ignore="UselessParent">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/t1"
                            android:text="@string/DemonTime"
                            android:textSize="35sp"
                            android:layout_gravity="center"
                            android:textColor="@color/white"/>
                        <TextView
                            android:id="@+id/location_latitude"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/setLatitude"
                            android:textSize="20sp"
                            android:layout_gravity="center"
                            android:textColor="@color/white"/>
                        <TextView
                            android:id="@+id/location_longitude"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/setLongitude"
                            android:textSize="20sp"
                            android:layout_gravity="center"
                            android:textColor="@color/white"/>

                        <TextView
                            android:id="@+id/temperature_text_view"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/setTemperature"
                            android:textSize="30sp"
                            android:layout_gravity="center"
                            android:textColor="@color/white"/>
                        <TextView
                            android:gravity="center"
                            android:id="@+id/location_description"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/setWeatherState"
                            android:textSize="30sp"
                            android:layout_gravity="center"
                            android:textColor="@color/white"/>
                        <TextView
                            android:gravity="center"
                            android:id="@+id/airquality_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/setAirPollution"
                            android:textSize="30sp"
                            android:layout_gravity="center"
                            android:textColor="@color/white"/>
                    </LinearLayout>
                </LinearLayout>

                <!-- 첫 번째 화면 하단 -->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:weightSum="6"
                    android:layout_margin="10dp"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:background="@drawable/round_top"
                        android:layout_marginBottom="1dp"
                        android:layout_weight="2"
                        tools:ignore="NestedWeights">
                        <TextView
                            android:id="@+id/forecastText"
                            android:padding="10dp"
                            android:layout_gravity="center_vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/setRainSnowState"
                            android:textSize="25sp"
                            android:layout_weight="1"
                            android:textColor="@color/white"
                            tools:ignore="InefficientWeight" />
                    </LinearLayout>

                    <HorizontalScrollView
                        android:scrollbars="none"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:background="@drawable/square"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="3">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:id="@+id/scRollTimeWeather"
                            android:orientation="horizontal"
                            tools:ignore="ScrollViewSize">

                        </LinearLayout>
                    </HorizontalScrollView>
                    <TextView
                        android:id="@+id/locationText"
                        android:layout_marginTop="1dp"
                        android:background="@drawable/round_bottom"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:text="@string/setLocalWeather"
                        android:layout_weight="1"
                        android:textColor="@color/white"
                        android:textSize="25sp"
                        android:gravity="center"/>
                </LinearLayout>

            </LinearLayout>


            <!-- Air Pollution -->
            <LinearLayout
                android:id="@+id/airPollution"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:layout_marginRight="10dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="10dp"
                        android:textSize="20sp"
                        android:textColor="#ffffff"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="미세먼지(PM 10)"/>
                    <LinearLayout
                        android:layout_margin="10dp"
                        android:id="@+id/Layout_pm10"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/round">
                        <TextView
                            android:id="@+id/pm10text"
                            android:layout_marginRight="10dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="10dp"
                            android:textSize="20sp"
                            android:textColor="#ffffff"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="pm10얘기"/>
                        <LinearLayout
                            android:layout_marginTop="10dp"
                            android:layout_gravity="center"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_weight="4">
                            <TextView
                                android:layout_marginRight="5dp"
                                android:layout_width="wrap_content"
                                android:layout_height="20dp"
                                android:gravity="center"
                                android:text="좋음0~30µg/㎥"
                                android:textColor="@color/white"
                                android:background="@drawable/air_round0"
                                android:textSize="11dp"
                                android:layout_weight="1"/>
                            <TextView
                                android:layout_marginRight="5dp"
                                android:layout_width="wrap_content"
                                android:layout_height="20dp"
                                android:gravity="center"
                                android:text="보통31~80µg/㎥"
                                android:textColor="@color/white"
                                android:background="@drawable/air_round1"
                                android:textSize="11dp"
                                android:layout_weight="1"/>
                            <TextView
                                android:layout_marginRight="5dp"
                                android:layout_width="wrap_content"
                                android:layout_height="20dp"
                                android:gravity="center"
                                android:text="나쁨81~150µg/㎥"
                                android:textColor="@color/white"
                                android:background="@drawable/air_round2"
                                android:textSize="11dp"
                                android:layout_weight="1"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="20dp"
                                android:gravity="center"
                                android:text="매우나쁨150µg/㎥~"
                                android:textColor="@color/white"
                                android:background="@drawable/air_round3"
                                android:textSize="11dp"
                                android:layout_weight="1"/>
                        </LinearLayout>
<!--                        <ImageView-->
<!--                            android:layout_marginRight="10dp"-->
<!--                            android:layout_marginLeft="10dp"-->
<!--                            android:layout_width="match_parent"-->
<!--                            android:layout_height="wrap_content"-->
<!--                            android:src="@drawable/pm10"-->
<!--                            />-->
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:layout_marginRight="10dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="10dp"
                        android:textSize="20sp"
                        android:textColor="#ffffff"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="미세먼지(PM 2.5)"/>
                    <LinearLayout
                        android:layout_margin="10dp"
                        android:id="@+id/Layout_pm2_5"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/round">
                        <TextView
                            android:id="@+id/pm2_5text"
                            android:layout_marginRight="10dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="10dp"
                            android:textSize="20sp"
                            android:textColor="#ffffff"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="pm2.5 얘기"/>
                        <LinearLayout
                            android:layout_marginTop="10dp"
                            android:layout_gravity="center"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_weight="4">
                            <TextView
                                android:layout_marginRight="5dp"
                                android:layout_width="wrap_content"
                                android:layout_height="20dp"
                                android:gravity="center"
                                android:text="좋음0~15µg/㎥"
                                android:textColor="@color/white"
                                android:background="@drawable/air_round0"
                                android:textSize="11dp"
                                android:layout_weight="1"/>
                            <TextView
                                android:layout_marginRight="5dp"
                                android:layout_width="wrap_content"
                                android:layout_height="20dp"
                                android:gravity="center"
                                android:text="보통16~35µg/㎥"
                                android:textColor="@color/white"
                                android:background="@drawable/air_round1"
                                android:textSize="11dp"
                                android:layout_weight="1"/>
                            <TextView
                                android:layout_marginRight="5dp"
                                android:layout_width="wrap_content"
                                android:layout_height="20dp"
                                android:gravity="center"
                                android:text="나쁨36~75µg/㎥"
                                android:textColor="@color/white"
                                android:background="@drawable/air_round2"
                                android:textSize="11dp"
                                android:layout_weight="1"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="20dp"
                                android:gravity="center"
                                android:text="매우나쁨76µg/㎥~"
                                android:textColor="@color/white"
                                android:background="@drawable/air_round3"
                                android:textSize="11dp"
                                android:layout_weight="1"/>
                        </LinearLayout>
<!--                        <ImageView-->
<!--                            android:layout_marginRight="10dp"-->
<!--                            android:layout_marginLeft="10dp"-->
<!--                            android:layout_width="match_parent"-->
<!--                            android:layout_height="wrap_content"-->
<!--                            android:src="@drawable/pm2_5"-->
<!--                            />-->
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:layout_marginRight="10dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="10dp"
                        android:textSize="20sp"
                        android:textColor="#ffffff"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="일산화탄소"/>
                    <LinearLayout
                        android:layout_margin="10dp"
                        android:id="@+id/Layout_CO"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/round">
                        <TextView
                            android:id="@+id/COtext"
                            android:layout_marginRight="10dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="10dp"
                            android:textSize="20sp"
                            android:textColor="#ffffff"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="일산화탄소 얘기"/>
                        <LinearLayout
                            android:layout_marginTop="10dp"
                            android:layout_gravity="center"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_weight="4">
                            <TextView
                                android:layout_marginRight="5dp"
                                android:layout_width="wrap_content"
                                android:layout_height="20dp"
                                android:gravity="center"
                                android:text="좋음~4400µg/㎥"
                                android:textColor="@color/white"
                                android:background="@drawable/air_round0"
                                android:textSize="11dp"
                                android:layout_weight="1"/>
                            <TextView
                                android:layout_marginRight="5dp"
                                android:layout_width="wrap_content"
                                android:layout_height="20dp"
                                android:gravity="center"
                                android:text="보통~9400µg/㎥"
                                android:textColor="@color/white"
                                android:background="@drawable/air_round1"
                                android:textSize="11dp"
                                android:layout_weight="1"/>
                            <TextView
                                android:layout_marginRight="5dp"
                                android:layout_width="wrap_content"
                                android:layout_height="20dp"
                                android:gravity="center"
                                android:text="나쁨~12400µg/㎥"
                                android:textColor="@color/white"
                                android:background="@drawable/air_round2"
                                android:textSize="11dp"
                                android:layout_weight="1"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="20dp"
                                android:gravity="center"
                                android:text="매우나쁨12400µg/㎥~"
                                android:textColor="@color/white"
                                android:background="@drawable/air_round3"
                                android:textSize="11dp"
                                android:layout_weight="1"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:layout_marginRight="10dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="10dp"
                        android:textSize="20sp"
                        android:textColor="#ffffff"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="오존"/>
                    <LinearLayout
                        android:layout_margin="10dp"
                        android:id="@+id/Layout_O3"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/round">
                        <TextView
                            android:id="@+id/O3text"
                            android:layout_marginRight="10dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="10dp"
                            android:textSize="20sp"
                            android:textColor="#ffffff"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="오존 얘기"/>
                        <LinearLayout
                            android:layout_marginTop="10dp"
                            android:layout_gravity="center"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_weight="4">
                            <TextView
                                android:layout_marginRight="5dp"
                                android:layout_width="wrap_content"
                                android:layout_height="20dp"
                                android:gravity="center"
                                android:text="좋음0~60µg/㎥"
                                android:textColor="@color/white"
                                android:background="@drawable/air_round0"
                                android:textSize="11dp"
                                android:layout_weight="1"/>
                            <TextView
                                android:layout_marginRight="5dp"
                                android:layout_width="wrap_content"
                                android:layout_height="20dp"
                                android:gravity="center"
                                android:text="보통60~100µg/㎥"
                                android:textColor="@color/white"
                                android:background="@drawable/air_round1"
                                android:textSize="11dp"
                                android:layout_weight="1"/>
                            <TextView
                                android:layout_marginRight="5dp"
                                android:layout_width="wrap_content"
                                android:layout_height="20dp"
                                android:gravity="center"
                                android:text="나쁨100~140µg/㎥"
                                android:textColor="@color/white"
                                android:background="@drawable/air_round2"
                                android:textSize="11dp"
                                android:layout_weight="1"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="20dp"
                                android:gravity="center"
                                android:text="매우나쁨140µg/㎥~"
                                android:textColor="@color/white"
                                android:background="@drawable/air_round3"
                                android:textSize="11dp"
                                android:layout_weight="1"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <!-- ForeCast -->
            <LinearLayout
                android:id="@+id/foreCast"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
                <ScrollView
                    android:scrollbars="none"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    tools:ignore="UselessParent">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:id="@+id/scRollDayweather"
                        tools:ignore="ScrollViewSize">

                    </LinearLayout>
                </ScrollView>
            </LinearLayout>


            <!-- ETC -->
            <LinearLayout
                android:id="@+id/Etc"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
                <ScrollView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <GridLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:columnCount="2"
                            android:rowCount="5">
                            <LinearLayout
                                android:id="@+id/etc1"
                                android:layout_width="0dp"
                                android:layout_height="200dp"
                                android:layout_margin="5dp"
                                android:layout_columnWeight="1"
                                android:layout_column="0"
                                android:layout_row="0"
                                android:background="@drawable/round"
                                android:minHeight="0dp"
                                android:minWidth="0dp"
                                android:layout_gravity="fill_horizontal"
                                android:orientation="horizontal">
                                <TextView
                                    android:id="@+id/sunset"
                                    android:text="일몰"
                                    android:layout_height="match_parent"
                                    android:layout_width="match_parent"
                                    android:layout_weight="1"
                                    android:textColor="#ffffff"
                                    android:textSize="20sp"
                                    android:gravity="center">
                                </TextView>
                                <TextView
                                    android:id="@+id/sunrise"
                                    android:text="일출"
                                    android:layout_height="match_parent"
                                    android:layout_width="match_parent"
                                    android:layout_weight="1"
                                    android:textColor="#ffffff"
                                    android:textSize="20sp"
                                    android:gravity="center"/>
                            </LinearLayout>
                            <LinearLayout
                                android:id="@+id/etc2"
                                android:layout_width="0dp"
                                android:layout_height="200dp"
                                android:layout_margin="5dp"
                                android:layout_columnWeight="1"
                                android:layout_column="1"
                                android:layout_row="0"
                                android:background="@drawable/round"
                                android:minHeight="0dp"
                                android:minWidth="0dp"
                                android:layout_gravity="fill_horizontal"
                                android:orientation="vertical"
                                >
                                <TextView
                                    android:id="@+id/windDeg"
                                    android:text="풍향"
                                    android:layout_height="match_parent"
                                    android:layout_width="match_parent"
                                    android:layout_weight="1"
                                    android:textColor="#ffffff"
                                    android:textSize="20sp"
                                    android:gravity="center"/>
                                <TextView
                                    android:id="@+id/windSpeed"
                                    android:text="풍속"
                                    android:layout_height="match_parent"
                                    android:layout_width="match_parent"
                                    android:layout_weight="1"
                                    android:textColor="#ffffff"
                                    android:textSize="20sp"
                                    android:gravity="center"/>
                            </LinearLayout>
                            <LinearLayout
                                android:id="@+id/etc3"
                                android:layout_width="0dp"
                                android:layout_height="200dp"
                                android:layout_margin="5dp"
                                android:layout_columnWeight="1"
                                android:layout_column="0"
                                android:layout_row="1"
                                android:background="@drawable/round"
                                android:minHeight="0dp"
                                android:minWidth="0dp"
                                android:layout_gravity="fill_horizontal"
                                android:orientation="vertical">
                                <TextView
                                    android:id="@+id/rainAmount"
                                    android:text="강우량"
                                    android:layout_height="match_parent"
                                    android:layout_width="match_parent"
                                    android:layout_weight="1"
                                    android:textColor="#ffffff"
                                    android:textSize="20sp"
                                    android:gravity="center"/>
                            </LinearLayout>
                            <LinearLayout
                                android:id="@+id/etc4"
                                android:layout_width="0dp"
                                android:layout_height="200dp"
                                android:layout_margin="5dp"
                                android:layout_columnWeight="1"
                                android:layout_column="1"
                                android:layout_row="1"
                                android:background="@drawable/round"
                                android:minHeight="0dp"
                                android:minWidth="0dp"
                                android:layout_gravity="fill_horizontal"
                                android:orientation="vertical">
                                <TextView
                                    android:id="@+id/feelTemp"
                                    android:text="체감온도"
                                    android:layout_height="match_parent"
                                    android:layout_width="match_parent"
                                    android:layout_weight="1"
                                    android:textColor="#ffffff"
                                    android:textSize="20sp"
                                    android:gravity="center"/>
                            </LinearLayout>
                            <LinearLayout
                                android:id="@+id/etc5"
                                android:layout_width="0dp"
                                android:layout_height="200dp"
                                android:layout_margin="5dp"
                                android:layout_columnWeight="1"
                                android:layout_column="0"
                                android:layout_row="2"
                                android:background="@drawable/round"
                                android:minHeight="0dp"
                                android:minWidth="0dp"
                                android:layout_gravity="fill_horizontal"
                                android:orientation="vertical">
                                <TextView
                                    android:id="@+id/humidity"
                                    android:text="습도"
                                    android:layout_height="match_parent"
                                    android:layout_width="match_parent"
                                    android:layout_weight="1"
                                    android:textColor="#ffffff"
                                    android:textSize="20sp"
                                    android:gravity="center"/>

                            </LinearLayout>
                            <LinearLayout
                                android:id="@+id/etc6"
                                android:layout_width="0dp"
                                android:layout_height="200dp"
                                android:layout_margin="5dp"
                                android:layout_columnWeight="1"
                                android:layout_column="1"
                                android:layout_row="2"
                                android:background="@drawable/round"
                                android:minHeight="0dp"
